doctype html

- var title = 'Project title'
- var socialList = ["fb", "inst"];

block vars
    
html
    head
        meta(charset="utf-8")
        meta(name="description", content="")
        meta(name="viewport", content="width=device-width, initial-scale=1, minimal-ui")
        title #{title}
        link(rel="icon" type="image/png" href="favicon.png" sizes="16x16")
        link(rel="stylesheet", href="styles/vendor.css")
        link(rel="stylesheet", href="styles/main.css")
    body
        include ../components/svg
        include ../components/modal
        block content
        include ../components/footer
        script(defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBzF9hHdclH4I3VRDc3uYG0iuqZThz2BZQ")
        script(defer src="scripts/vendor.js")
        script(defer src="scripts/app.js")
